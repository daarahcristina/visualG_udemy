Algoritmo "consumo"

Var

   distancia, media, combustivel : real

Inicio

   escreva("Distancia percorrida: ")
   leia(distancia)
   escreva("Combustivel gasto: ")
   leia(combustivel)
   
   media <- distancia / combustivel
   
   escreva("Consumo medio = ", media:5:3)
Fimalgoritmo